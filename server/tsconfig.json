{
    "compilerOptions": {
      /* Visit https://aka.ms/tsconfig.json to read more about this file */
  
      "target": "es2016",
      "esModuleInterop": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "forceConsistentCasingInFileNames": true,
      "strict": true,   
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "paths": {
        "*": [
            "node_modules/*"
        ]
      }
    },
    "exclude": ["node_modules"],
  }